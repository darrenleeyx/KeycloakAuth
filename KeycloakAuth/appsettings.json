{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "AuthorizationUrl": "http://localhost:8080/realms/KeycloakAuth/protocol/openid-connect/auth"
  },
  "Authentication": {
    "MetadataAddress": "http://localhost:8080/realms/KeycloakAuth/.well-known/openid-configuration",
    "ValidIssuer": "http://localhost:8080/auth/realms/KeycloakAuth",
    "Audience": "account"
  },
  "AllowedHosts": "*"
}
